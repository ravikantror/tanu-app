<%= link_to 'Logout',destroy_user_session_path,:method => :delete %>

<%#*<div class="container">%>
<h1>All Users Posts</h1>
<table id="posts">
  <thead>
    <tr>
      <th>Name</th>
      <th>User_Id &nbsp;&nbsp;</th>
      <th>Post_Id</th>
      <th>Description</th>
      <th>Post_type</th>
      <th colspan="4"></th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post|%>
      <tr class="row" , id="row-<%= post.id %>">
      <td><%= post.name %></td>
        <td><%= post.user_id %></td>
        <td><%= post.id %></td>
        <td><%= post.des %></td>
        <td><%= post.type %></td>
        <td><%= link_to 'Show',post_path(post), :remote => true %></td>
        <td><%= link_to 'Edit', edit_post_path(post),:remote => true %></td>
        <td><%= link_to 'Destroy', delete_post_path(post),:remote => true,
              data: { confirm: 'Are you sure?' } %></td>
</tr>

    <%#= render :partial => 'post' %>
    <% end %>
<%#*<tr><%= render @post%>
  </tbody>
</table>
<%#= render @posts %>
<%#*</div>%>
