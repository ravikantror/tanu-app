<%= link_to 'Logout',destroy_user_session_path,:method => :delete %>
<h1>All Users Posts</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th>Description</th>
      <th>Post_type</th>
      <th colspan="4"></th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.name %></td>
        <td><%= post.user_id %></td>
        <td><%= post.des %></td>
        <td><%= post.type %></td>
        <td><%= link_to 'Show',post_path(post) %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', delete_post_path(post),
              data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'New Post', new_post_path %>


